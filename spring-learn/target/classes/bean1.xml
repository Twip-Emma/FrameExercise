<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="user" class="test.User"></bean>
    <bean id="book" class="test.Book">
        <property name="bookName" value="易筋经"></property>
        <property name="price" value="888"></property>
    </bean>
    <bean id="orders" class="test.Orders">
        <constructor-arg name="orderName" value="电脑"></constructor-arg>
        <constructor-arg name="orderAdd" value="中国"></constructor-arg>
    </bean>
</beans>