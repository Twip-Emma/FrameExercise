<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
   <welcome-file-list>
     <welcome-file>index.jsp</welcome-file>
     <welcome-file>index.html</welcome-file>
   </welcome-file-list>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>HelloWorld1</servlet-name>
    <servlet-class>main.java.myServlet.ServletInfo</servlet-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>timeJunp</servlet-name>
    <servlet-class>main.java.myServlet.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>headerGetter</servlet-name>
    <servlet-class>main.java.myServlet.RequestHeaderServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>main.java.myServlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login2</servlet-name>
    <servlet-class>main.java.myServlet.RequestParamsServlet</servlet-class>
  </servlet>
  <!--Mapping-->
  <servlet-mapping>
    <servlet-name>HelloWorld1</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>timeJunp</servlet-name>
    <url-pattern>/junp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>headerGetter</servlet-name>
    <url-pattern>/header</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/userLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login2</servlet-name>
    <url-pattern>/userLogin2</url-pattern>
  </servlet-mapping>
</web-app>
